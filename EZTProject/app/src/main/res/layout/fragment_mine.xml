<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/e_act_bg" >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="@dimen/inc_dp_100"
                android:background="@drawable/e_index_top_bg"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/inc_dp_20"
                    android:layout_weight="1.0"
                    android:gravity="center"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:gravity="center_vertical"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@id/user_info_name"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:textColor="@color/inc_item_background"
                            android:textSize="@dimen/inc_dp_16" />

                        <TextView
                            android:id="@id/user_info_type"
                            android:layout_width="wrap_content"
                            android:layout_height="fill_parent"
                            android:layout_marginLeft="@dimen/inc_dp_5"
                            android:background="@drawable/bk_approve"
                            android:drawableLeft="@drawable/ic_approve002"
                            android:drawablePadding="@dimen/inc_dp_5"
                            android:gravity="center_vertical"
                            android:paddingLeft="@dimen/inc_dp_5"
                            android:text="@string/e_text_owner"
                            android:textColor="@color/inc_item_background"
                            android:textSize="@dimen/inc_dp_10" />
                    </LinearLayout>

                    <TextView
                        android:id="@id/user_info_phone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:layout_marginTop="@dimen/inc_dp_10"
                        android:textColor="@color/inc_item_background"
                        android:textSize="@dimen/inc_dp_14" />
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="3.0"
                    android:visibility="visible" >

                    <Button
                        android:id="@id/e_modify"
                        style="@style/index_bottom_modify"
                        android:layout_centerInParent="true"
                        android:background="@drawable/shape_epay_btn"
                        android:paddingLeft="@dimen/inc_dp_5"
                        android:paddingRight="@dimen/inc_dp_5"
                        android:text="@string/e_modify"
                        android:visibility="gone" />
                </RelativeLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="65.0dip"
                android:background="@color/inc_item_background"
                android:orientation="horizontal" >

                <RelativeLayout
                    android:id="@id/index_type_1_pre"
                    style="@style/index_image_btn_pre"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.0" >

                    <TextView
                        android:id="@id/index_type_1"
                        style="@style/index_me_text_top"
                        android:text="@string/e_me_current_count" />

                    <TextView
                        android:id="@id/index_type_1_value"
                        style="@style/index_me_text_bottom"
                        android:layout_below="@id/index_type_1"
                        android:text="" />
                </RelativeLayout>

                <View
                    android:layout_width="@dimen/inc_dp_0_5"
                    android:layout_height="fill_parent"
                    android:background="@color/page_content" />

                <RelativeLayout
                    android:id="@id/rel_jifen"
                    style="@style/index_image_btn_pre"
                    android:layout_gravity="center_vertical"
                    android:layout_weight="1.0" >

                    <TextView
                        android:id="@id/index_type_3"
                        style="@style/index_me_text_top"
                        android:text="@string/e_me_person_1" />

                    <TextView
                        android:id="@id/index_type_3_value"
                        style="@style/index_me_text_bottom"
                        android:layout_below="@id/index_type_3"
                        android:text="" />
                </RelativeLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/inc_dp_0_5"
                android:background="@color/inc_complete_line" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginTop="@dimen/inc_dp_10"
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/inc_item_background"
                    android:orientation="vertical" >

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:background="@color/inc_complete_line" />

                    <RelativeLayout
                        android:id="@id/onwer_me_rela_1"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_44" >

                        <RelativeLayout
                            android:id="@id/onwer_me_rel_1"
                            android:layout_width="@dimen/inc_dp_50"
                            android:layout_height="@dimen/inc_dp_44"
                            android:layout_alignParentLeft="true" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/ic_tixing" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10.0dip"
                            android:layout_toRightOf="@id/onwer_me_rel_1"
                            android:text="@string/e_balance_notify"
                            android:textColor="@color/black"
                            android:textSize="@dimen/inc_dp_16" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/inc_dp_10"
                            android:src="@drawable/ic_enter" />
                    </RelativeLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:layout_marginLeft="@dimen/inc_dp_50"
                        android:background="@color/inc_complete_line" />

                    <RelativeLayout
                        android:id="@id/me_rela_0_1"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_44" >

                        <RelativeLayout
                            android:id="@id/me_rel_0_1"
                            android:layout_width="@dimen/inc_dp_50"
                            android:layout_height="@dimen/inc_dp_44"
                            android:layout_alignParentLeft="true" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/ic_personal006" />
                        </RelativeLayout>

                        <TextView
                            android:id="@id/yhq_tx"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10.0dip"
                            android:layout_toRightOf="@id/me_rel_0_1"
                            android:text="@string/e_my_youhuiquan"
                            android:textColor="@color/black"
                            android:textSize="@dimen/inc_dp_16" />

                        <ImageView
                            android:id="@id/un_consume"
                            android:layout_width="6.0dip"
                            android:layout_height="6.0dip"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10.0dip"
                            android:layout_toRightOf="@id/yhq_tx"
                            android:scaleType="fitCenter"
                            android:src="@drawable/icon_notification"
                            android:visibility="gone" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/inc_dp_10"
                            android:src="@drawable/ic_enter" />
                    </RelativeLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:layout_marginLeft="@dimen/inc_dp_50"
                        android:background="@color/inc_complete_line" />

                    <RelativeLayout
                        android:id="@id/me_rela_service"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_44" >

                        <RelativeLayout
                            android:id="@id/me_rel_s"
                            android:layout_width="@dimen/inc_dp_50"
                            android:layout_height="@dimen/inc_dp_44"
                            android:layout_alignParentLeft="true" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/icon_kefu" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10.0dip"
                            android:layout_toRightOf="@id/me_rel_s"
                            android:text="@string/e_my_service"
                            android:textColor="@color/black"
                            android:textSize="@dimen/inc_dp_16" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/inc_dp_10"
                            android:src="@drawable/ic_enter" />
                    </RelativeLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:background="@color/inc_complete_line" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/inc_dp_10"
                    android:background="@color/inc_item_background"
                    android:orientation="vertical" >

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:background="@color/inc_complete_line" />

                    <RelativeLayout
                        android:id="@id/me_mange_account"
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_44" >

                        <RelativeLayout
                            android:id="@id/mange_account"
                            android:layout_width="@dimen/inc_dp_50"
                            android:layout_height="@dimen/inc_dp_44"
                            android:layout_alignParentLeft="true" >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerInParent="true"
                                android:layout_centerVertical="true"
                                android:src="@drawable/ic_personal007" />
                        </RelativeLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginLeft="10.0dip"
                            android:layout_toRightOf="@id/mange_account"
                            android:text="@string/e_mange_acount"
                            android:textColor="@color/black"
                            android:textSize="@dimen/inc_dp_16" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/inc_dp_10"
                            android:src="@drawable/ic_enter" />
                    </RelativeLayout>

                    <View
                        android:layout_width="fill_parent"
                        android:layout_height="@dimen/inc_dp_0_5"
                        android:background="@color/inc_complete_line" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/inc_dp_0_5"
                android:layout_marginTop="@dimen/inc_dp_20"
                android:background="@color/inc_complete_line" />

            <RelativeLayout
                android:id="@id/onwer_me_rela_logout"
                android:layout_width="fill_parent"
                android:layout_height="@dimen/inc_dp_44"
                android:background="@color/inc_item_background" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:text="@string/e_logout"
                    android:textColor="@color/black"
                    android:textSize="@dimen/inc_dp_16" />
            </RelativeLayout>

            <View
                android:layout_width="fill_parent"
                android:layout_height="@dimen/inc_dp_0_5"
                android:background="@color/inc_complete_line" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>